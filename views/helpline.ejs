<%- include("partials/header"); -%>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">State</th>
      <th scope="col">Number</th>
    </tr>
  </thead>
  <tbody>
<% contactArray.forEach((contact,i)=>{ %>
    <tr>
      <th scope="row"><%=i+1%></th>
      <td><%=contact.loc%></td>
      <td><%=contact.number%></td>
    </tr>
<% }); %>
  </tbody>
</table>

<div class="row">
  <div class="col-sm-3">
    Phone: <%=primaryContacts.number%>
  </div>
  <div class="col-sm-3">
    Tollfree No.: 1075
  </div>
  <div class="col-sm-3">
    Email: <%=primaryContacts.email%>
  </div>
  <div class="col-sm-1">
    <a href="<%=primaryContacts.twitter%>"><i class="fab fa-twitter" aria-hidden="true"></i></a>
  </div>
  <div class="col-sm-1">
    <a href="<%=primaryContacts.facebook%>"><i class="fab fa-facebook" aria-hidden="true"></i></a>
  </div>
</div>

<%- include("partials/footer"); -%>
